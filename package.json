{
  "name": "org_model",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "start": "tsnd",
    "test": "jest -o",
    "test:watch": "yarn test --watch",
    "test:coverage": "jest --all --coverage",
    "doc": "typedoc --out ./reports/docs src/",
    "plato": "ts-plato -r -n -x '\\.(spec|test)\\.' -t 'Source Analysis of org_model' -d ./reports/plato src",
    "reports": "yarn doc; yarn plato; yarn test:coverage",
    "dbup": "db-migrate up",
    "dbdown": "db-migrate down",
    "dbreset": "db-migrate reset"
  },
  "dependencies": {
    "@types/pg": "^8.6.1",
    "@types/pg-format": "^1.0.2",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "pg": "^8.6.0",
    "pg-format": "^1.0.4",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-html-reporter": "^3.4.1",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.8",
    "ts-plato": "^0.0.6",
    "typedoc": "^0.21.2"
  }
}
